<p id="notice"><%= notice %></p>

<h1>Alumni</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Degree</th>
      <th>Divsion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alums.each do |alum| %>
      <tr>
        <td><%= alum.name %></td>
        <td><%= alum.email %></td>
        <td><%= alum.degree %></td>
        <td><%= alum.divsion %></td>
        <td><%= link_to 'Show', alum %></td>
        <td><%= link_to 'Edit', edit_alum_path(alum) %></td>
        <td><%= link_to 'Delete', alum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Add Contact', new_alum_path %>

