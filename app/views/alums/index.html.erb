<p id="notice"><%= notice %></p>

<h1>Alums</h1>

<table>
  <thead>
    <tr>
      <th>Mem</th>
      <th>Mem isactive</th>
      <th>Mem name</th>
      <th>Mem email</th>
      <th>Mem points</th>
      <th>Mem dept</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alums.each do |alum| %>
      <tr>
        <td><%= alum.Mem_id %></td>
        <td><%= alum.Mem_isActive %></td>
        <td><%= alum.Mem_name %></td>
        <td><%= alum.Mem_email %></td>
        <td><%= alum.Mem_Points %></td>
        <td><%= alum.Mem_Dept %></td>
        <td><%= link_to 'Show', alum %></td>
        <td><%= link_to 'Edit', edit_alum_path(alum) %></td>
        <td><%= link_to 'Destroy', alum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Alum', new_alum_path %>
