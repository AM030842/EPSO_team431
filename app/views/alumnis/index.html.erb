<p id="notice"><%= notice %></p>

<h1>Alumnis</h1>

<table>
  <thead>
    <tr>
      <th>Mem id</th>
      <th>Mem email</th>
      <th>Mem name</th>
      <th>Mem degree</th>
      <th>Mem dept</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alumnis.each do |alumni| %>
      <tr>
        <td><%= alumni.mem_Id %></td>
        <td><%= alumni.mem_Email %></td>
        <td><%= alumni.mem_Name %></td>
        <td><%= alumni.mem_Degree %></td>
        <td><%= alumni.mem_Dept %></td>
        <td><%= link_to 'Show', alumni %></td>
        <td><%= link_to 'Edit', edit_alumni_path(alumni) %></td>
        <td><%= link_to 'Destroy', alumni, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Alumni', new_alumni_path %>
