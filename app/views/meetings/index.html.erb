<p id="notice"><%= notice %></p>

<h1>Meetings</h1>

<table>
  <thead>
    <tr>
      <th>Meeting</th>
      <th>Meeting pts</th>
      <th>Meeting title</th>
      <th>Meeting time</th>
      <th>Meeting desc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meetings.each do |meeting| %>
      <tr>
        <td><%= meeting.meeting_id %></td>
        <td><%= meeting.meeting_pts %></td>
        <td><%= meeting.meeting_title %></td>
        <td><%= meeting.meeting_time %></td>
        <td><%= meeting.meeting_desc %></td>
        <td><%= link_to 'Show', meeting %></td>
        <td><%= link_to 'Edit', edit_meeting_path(meeting) %></td>
        <td><%= link_to 'Destroy', meeting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Meeting', new_meeting_path %>

